<?xml version="1.0" encoding="UTF-8"?>
<interface domain="cnchi">
	<requires lib="gtk+" version="3.18"/>
	<object class="GtkBox" id="features">
		<property name="visible">True</property>
		<property name="can_focus">False</property>
		<property name="orientation">vertical</property>
		<!--<property name="margin_top">30px</property>
		<property name="margin_bottom">30px</property>
		<property name="margin_left">50px</property>
		<property name="margin_right">50px</property>-->
		<property name="hexpand">True</property>
		<property name="vexpand">True</property>
		<child>
			<object class="GtkScrolledWindow" id="scrolledwindow">
				<property name="visible">True</property>
				<property name="can_focus">True</property>
				<property name="hexpand">True</property>
				<property name="vexpand">True</property>
				<property name="hscrollbar_policy">never</property>
				<property name="shadow_type">in</property>
				<child>
					<object class="GtkListBox" id="listbox">
						<property name="visible">True</property>
						<property name="can_focus">True</property>
					</object>
				</child>
			</object>
			<packing>
				<property name="expand">True</property>
				<property name="fill">True</property>
				<property name="position">0</property>
			</packing>
		</child>
	</object>
</interface>
