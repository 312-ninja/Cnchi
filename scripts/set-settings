dbus-launch gsettings set org.mate.search-tool search-history {'gsearchtool-file-entry': ['netbeans']}
dbus-launch gsettings set org.mate.caja.desktop font 'Sans 10'
dbus-launch gsettings set org.mate.caja.icon-view captions ['none', 'size', 'date_modified']
dbus-launch gsettings set org.mate.caja.list-view default-column-order ['name', 'size', 'type', 'date_modified', 'date_accessed', 'group', 'where', 'mime_type', 'octal_permissions', 'owner', 'permissions']
dbus-launch gsettings set org.mate.caja.list-view default-visible-columns ['name', 'size', 'type', 'date_modified']
