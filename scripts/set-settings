dbus-launch gsettings set org.mate.caja.list-view default-column-order ['name', 'size', 'type', 'date_modified', 'date_accessed', 'group', 'where', 'mime_type', 'octal_permissions', 'owner', 'permissions']
dbus-launch gsettings set org.mate.caja.list-view default-visible-columns ['name', 'size', 'type', 'date_modified']
dbus-launch gsettings set org.gnome.desktop.screensaver lock-delay uint32 0
dbus-launch gsettings set org.gnome.desktop.screensaver logout-delay uint32 7200
dbus-launch gsettings set org.mate.pluma.plugins.filebrowser.on-load root 'file:///'
dbus-launch gsettings set org.mate.pluma.plugins.filebrowser.on-load virtual-root 'file:///etc/gconf/2'
dbus-launch gsettings set org.mate.power-manager check-type-cpu true
