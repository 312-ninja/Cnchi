dbus-launch gsettings set org.mate.Marco.keybinding-commands command-1 'sudo killall X'
dbus-launch gsettings set org.mate.Marco.keybinding-commands command-window-screenshot 'mate-screenshot --window'
dbus-launch gsettings set org.mate.mucharmap.WindowState size (-1, -1)
dbus-launch gsettings set org.mate.screensaver logout-command '/usr/bin/mate-session-save --logout-dialog  '
dbus-launch gsettings set org.mate.screensaver logout-enabled true
dbus-launch gsettings set org.mate.screensaver themes @as []
dbus-launch gsettings set org.mate.pluma.plugins.filebrowser.on-load root 'file:///'
dbus-launch gsettings set org.mate.pluma.plugins.filebrowser.on-load virtual-root 'file:///etc/gconf/2'
dbus-launch gsettings set org.mate.caja.window-state geometry '800x550+157+136'
dbus-launch gsettings set org.mate.caja.window-state sidebar-width 159
dbus-launch gsettings set org.mate.control-center cc-actions-list ['Change Theme;mate-appearance-properties.desktop', 'Set Preferred Applications;mate-default-applications-properties.desktop']]

fi

